<?xml version="1.0"?>

<!DOCTYPE faces-config PUBLIC
  "-//Sun Microsystems, Inc.//DTD JavaServer Faces Config 1.0//EN"
  "http://java.sun.com/dtd/web-facesconfig_1_0.dtd"
[
 <!ELEMENT idegaweb-info ANY>
 <!ELEMENT builder-visible (#PCDATA)>
 <!ELEMENT object-type (#PCDATA)>
 <!ELEMENT component-metadata ANY>
 <!ATTLIST component-metadata
     component-type CDATA #IMPLIED>
 <!ELEMENT facet-metadata ANY>
 <!ELEMENT attribute-metadata ANY>
 <!ELEMENT property-metadata ANY>
 <!ELEMENT accepts-child-components (#PCDATA)>
 <!ELEMENT attribute-values (#PCDATA)>
 <!ELEMENT base-component-type  (#PCDATA)>
 <!ELEMENT deprecated  (#PCDATA)>
 <!ELEMENT display-order  (#PCDATA)>
 <!ELEMENT expert  (#PCDATA)>
 <!ELEMENT favorite-property  (#PCDATA)>
 <!ELEMENT group  (#PCDATA)>
 <!ELEMENT help-topic  (#PCDATA)>
 <!ELEMENT hidden  (#PCDATA)>
 <!ELEMENT initial-value  (#PCDATA)>
 <!ELEMENT preferred  (#PCDATA)>
 <!ELEMENT preferred-child-tags  (#PCDATA)>
 <!ELEMENT property-editor  (#PCDATA)>
 <!ELEMENT short-description  (#PCDATA)>
 <!ELEMENT writable  (#PCDATA)> 
]
>
<faces-config>
	<component>
		<component-type>com.idega.Navigation</component-type>
		<component-class>com.idega.block.navigation.presentation.Navigation</component-class>
		<property>
			<display-name>Style class</display-name>
			<property-name>styleClass</property-name>
			<property-class>java.lang.String</property-class>
		</property>
		<property>
			<display-name>ID</display-name>
			<property-name>id</property-name>
			<property-class>java.lang.String</property-class>
		</property>
		<property>
			<display-name>Show root</display-name>
			<property-name>showRoot</property-name>
			<property-class>boolean</property-class>
		</property>
		<property>
			<display-name>Open all nodes</display-name>
			<property-name>openAllNodes</property-name>
			<property-class>boolean</property-class>
		</property>
		<property>
			<display-name>Hide sub pages</display-name>
			<property-name>hideSubPages</property-name>
			<property-class>boolean</property-class>
		</property>
		<property>
			<display-name>Root page</display-name>
			<property-name>rootPageID</property-name>
			<property-class>int</property-class>
		</property>
		<property>
			<display-name>Facelet path</display-name>
			<property-name>faceletPath</property-name>
			<property-class>java.lang.String</property-class>
		</property>
		<property>
			<display-name>Facelet item path</display-name>
			<property-name>faceletItemPath</property-name>
			<property-class>java.lang.String</property-class>
		</property>
	</component>
	
	<component>
		<component-type>com.idega.BreadCrumbs</component-type>
		<component-class>com.idega.block.navigation.presentation.BreadCrumbs</component-class>
		<property>
			<display-name>Style class</display-name>
			<property-name>styleClass</property-name>
			<property-class>java.lang.String</property-class>
		</property>
		<property>
			<display-name>ID</display-name>
			<property-name>id</property-name>
			<property-class>java.lang.String</property-class>
		</property>
		<property>
			<display-name>Hide category pages</display-name>
			<property-name>hideCategoryPages</property-name>
			<property-class>boolean</property-class>
		</property>
		<property>
			<display-name>Root page</display-name>
			<property-name>rootPageID</property-name>
			<property-class>int</property-class>
		</property>
		<property>
			<display-name>Facelet path</display-name>
			<property-name>faceletPath</property-name>
			<property-class>java.lang.String</property-class>
		</property>
		<property>
			<display-name>Divider</display-name>
			<property-name>divider</property-name>
			<property-class>java.lang.String</property-class>
		</property>
	</component>
	
	<component>
		<component-type>com.idega.CurrentPage</component-type>
		<component-class>com.idega.block.navigation.presentation.CurrentPageText</component-class>
	</component>
</faces-config>